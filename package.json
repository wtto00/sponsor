{
  "name": "sponsor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite-ssg build",
    "preview": "vite preview",
    "lint": "eslint --fix --color **/*.{ts,vue}",
    "format": "prettier . --write --ignore-unknown",
    "tscheck": "vue-tsc --noEmit --pretty -p ./tsconfig.json",
    "prepare": "husky install && husky set .husky/pre-commit \"npx lint-staged\""
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.8.2",
    "@unhead/vue": "^1.7.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "typescript": "^5.2.2",
    "unocss": "^0.56.5",
    "vite": "^4.4.5",
    "vite-ssg": "^0.23.3",
    "vue-tsc": "^1.8.5"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "eslint": "^8.50.0",
    "eslint-config-standard-with-typescript": "^39.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "*.{ts,vue}": [
      "eslint --fix"
    ]
  }
}
